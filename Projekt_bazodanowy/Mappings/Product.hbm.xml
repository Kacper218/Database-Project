<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Projekt_bazodanowy" namespace ="Projekt_bazodanowy.Models">

	<class name="Product">
		<id name="ID">
			<generator class="native"/>
		</id>
		<property name="Name"/>
		<property name="Units"/>
		<property name="Price"/>
	</class>

	<class name="Klienci">
		<id name="IDKlienta">
			<generator class="native"/>
		</id>
		<property name="ImieNazwisko"/>
		<property name="NazwaFirmy"/>
		<property name="Email"/>
	</class>

	<class name="Produkty">
		<id name="IDProduktu">
			<generator class="native"/>
		</id>
		<property name="Nazwa"/>
		<property name="CenaAktualna"/>
		<property name="Dostepnosc"/>
	</class>


</hibernate-mapping>